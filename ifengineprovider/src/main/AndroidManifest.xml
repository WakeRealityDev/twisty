<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="com.wakereality.ifengineprovider"
>

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true"
    >

        <receiver android:name=".EngineLaunchBroadcastReceiver">
            <intent-filter>
                <action android:name="interactivefiction.enginemeta.runstory" />
                <action android:name="interactivefiction.enginemeta.close" />
                <action android:name="interactivefiction.enginemeta.storiesfoundrequest" />
                <action android:name="interactivefiction.enginemeta.commandexecuterequest" />
                <action android:name="interactivefiction.enginemeta.storydownloaded" />
                <action android:name="interactivefiction.engine.automatch" />
                <action android:name="interactivefiction.engine.glulx" />
                <action android:name="interactivefiction.engine.zmachine" />
            </intent-filter>
        </receiver>

    </application>

</manifest>
